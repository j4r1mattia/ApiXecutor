{
  "env": {
    "browser": false,
    "es2021": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "standard"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "space-before-function-paren": "off",
    "generator-star-spacing": "off",
    "yield-star-spacing": "off"
  }
}
